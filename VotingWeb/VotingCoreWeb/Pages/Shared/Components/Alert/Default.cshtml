@model VotingCoreWeb.Infrastructure.AlertModel

@if (Model != null)
{
    <div class="alert alert-dismissible @Model.Class" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h5 class="alertText"><b>@(Model.Title)</b> @Model.Message</h5>
    </div>
}